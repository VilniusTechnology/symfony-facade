{
    "name": "lmikelionis/symfonys-facade",
    "description": "Symfonys facade for The Laravel Framework.",
    "keywords": ["framework", "laravel", "symfony"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "5.0.*"
    },
    "require-dev": {
        "ongr/elasticsearch-bundle": "~0.1",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "phpmd/phpmd" : "@stable",
        "sebastian/phpcpd": "~2.0",
        "sebastian/phpdcd": "~1.0",
        "phploc/phploc": "~2.0",
        "ongr/ongr-strict-standard": "2.*",
        "squizlabs/php_codesniffer": "2.*",
        "orchestra/testbench": "3.0.*",
        "illuminate/html": "~5.0",
        "laravel/laravel": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "VilniusTechnology\\SymfonysFacade\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
